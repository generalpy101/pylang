var i = 0;

while (i < 10) {
    if (i == 2) {
        i = i + 1;
        print ("skipping 2");
        continue;
    }
    if (i == 5) {
        print ("breaking at 5");
        break;
    }
    print(i);
    i = i + 1;
}

for (var j = 0; j < 10; j = j + 1) {
    // Break at 5
    if (j == 5) {
        print ("breaking at 5");
        break;
    }
    print(j);
}